{
  "data": {
    "format": "dummy",
    "indexrange": 148,
    "numfield": 13,
    "validateratio": 0.1
  },
  "model": {
    "modeltype": "T_FLOAT_SPARSE",
    "modelsize": 148
  },
  "train": {
    "epoch": 5,
    "numupdateperepoch": 10,
    "lr": 1.0,
    "decay": 0.01
  },
  "default_optimizer": "Momentum",
  "layers": [
    {
      "name": "embedding",
      "type": "embedding",
      "numfactors": 8,
      "outputdim": 104,
      "optimizer": {
        "type": "momentum",
        "momentum": 0.9,
        "reg2": 0.01
      }
    },
    {
      "name": "deep",
      "type": "FCLayer",
      "outputdims": [
        104,
        104,
        104
      ],
      "transfuncs": [
        "relu",
        "relu",
        "relu"
      ],
      "inputlayer": "embedding"
    },
    {
      "name": "cross1",
      "type": "CrossLayer",
      "outputdim": 104,
      "inputlayers": [
        "embedding",
        "embedding"
      ]
    },
    {
      "name": "cross2",
      "type": "CrossLayer",
      "outputdim": 104,
      "inputlayers": [
        "embedding",
        "cross1"
      ]
    },
    {
      "name": "cross3",
      "type": "CrossLayer",
      "outputdim": 104,
      "inputlayers": [
        "embedding",
        "cross2"
      ]
    },
    {
      "name": "concat",
      "type": "ConcatLayer",
      "outputdim": 208,
      "inputlayers": [
        "deep",
        "cross3"
      ]
    },
    {
      "name": "lr",
      "type": "FCLayer",
      "outputdims": [
        1
      ],
      "transfuncs": [
        "sigmoid"
      ],
      "inputlayer": "concat"
    },
    {
      "name": "simplelosslayer",
      "type": "losslayer",
      "lossfunc": "logloss",
      "inputlayer": "lr"
    }
  ]
}
