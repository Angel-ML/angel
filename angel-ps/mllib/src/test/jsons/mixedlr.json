{
  "data": {
    "format": "libsvm",
    "indexrange": -1,
    "validateratio": 0.1,
    "sampleratio": 1.0
  },
  "train": {
    "epoch": 10,
    "lr": 0.5
  },
  "model": {
    "modeltype": "T_FLOAT_SPARSE"
  },
  "default_optimizer": {
    "type": "momentum",
    "momentum": 0.9,
    "reg2": 0.01
  },
  "layers": [
    {
      "name": "sigmoid",
      "type": "simpleinputlayer",
      "outputdim": 10,
      "transfunc": "sigmoid"
    },
    {
      "name": "softmax",
      "type": "simpleinputlayer",
      "outputdim": 10,
      "transfunc": "softmax"
    },
    {
      "name": "dotpooling",
      "type": "dotpooling",
      "outputdim": 1,
      "inputlayers": [
        "sigmoid",
        "softmax"
      ]
    },
    {
      "name": "simplelosslayer",
      "type": "losslayer",
      "lossfunc": "CrossEntropyLoss",
      "inputlayer": "dotpolling"
    }
  ]
}
