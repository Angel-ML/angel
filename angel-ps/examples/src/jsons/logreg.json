{
  "data": {
    "format": "libsvm",
    "indexrange": -1,
    "validateratio": 0.1,
    "sampleratio": 1.0
  },
  "train": {
    "epoch": 10,
    "lr": 0.5
  },
  "model": {
    "modeltype": "T_FLOAT_SPARSE"
  },
  "default_optimizer": {
    "type": "momentum",
    "momentum": 0.9,
    "reg2": 0.001
  },
  "layers": [
    {
      "name": "wide",
      "type": "simpleinputlayer",
      "outputdim": 1,
      "transfunc": "identity"
    },
    {
      "name": "simplelosslayer",
      "type": "simplelosslayer",
      "lossfunc": "logloss",
      "inputlayer": "wide"
    }
  ]
}
